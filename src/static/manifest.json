{
  "manifest_version": 2,
  "version": "0.0.0",
  "name": "",
  "short_name": "BROWSER_EXTENSION_TEMPLATE",
  "description": "",
  "icons": {
    "128": "img/icon-128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "clipboardWrite",
    "clipboardRead",
    "notifications",
    "background",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "img/icon-disabled-128.png"
  },
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start",
      "js": [
        "browser-polyfill.min.js",
        "content.js"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "https://*.wildlink.me/*"
    ]
  }
}
